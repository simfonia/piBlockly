<category id="category_arduino" name="%{BKY_ARDUINO_CATEGORY}" colour="%{BKY_ARDUINO_HUE}">
    <category id="category_arduino_structure" name="%{BKY_ARDUINO_STRUCTURE_CATEGORY}" colour="%{BKY_ARDUINO_HUE}">
        <block type="initializes_setup"></block>
        <block type="initializes_loop"></block>
    </category>
    <category id="category_arduino_io" name="%{BKY_ARDUINO_IO_CATEGORY}" colour="%{BKY_ARDUINO_HUE}">
        <block type="arduino_pin_mode">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
            <value name="MODE">
                <shadow type="arduino_pin_mode_mode_shadow"></shadow>
            </value>
        </block>
        <block type="arduino_digital_read">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
        </block>
        <block type="arduino_digital_write">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
        </block>
        <block type="arduino_analog_read">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN">A0</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_analog_write">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN">~</field>
                </shadow>
            </value>
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
    </category>
    <category id="category_arduino_time" name="%{BKY_ARDUINO_TIME_CATEGORY}" colour="%{BKY_ARDUINO_HUE}"></category>
    <category id="category_arduino_serial" name="%{BKY_ARDUINO_SERIAL_CATEGORY}" colour="%{BKY_ARDUINO_HUE}"></category>
</category>
