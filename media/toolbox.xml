<category id="category_arduino" name="%{BKY_ARDUINO_CATEGORY}" categorystyle="arduino_category">
    <category id="category_arduino_structure" name="%{BKY_ARDUINO_STRUCTURE_CATEGORY}" categorystyle="arduino_structure_category">
        <block type="initializes_setup"></block>
        <block type="initializes_loop"></block>
    </category>
    <category id="category_arduino_io" name="%{BKY_ARDUINO_IO_CATEGORY}" categorystyle="arduino_io_category">
        <block type="arduino_pin_mode">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
            <value name="MODE">
                <shadow type="arduino_pin_mode_mode_shadow"></shadow>
            </value>
        </block>
        <block type="arduino_digital_read">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
        </block>
        <block type="arduino_digital_write">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
        </block>
        <block type="arduino_analog_read">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN">A0</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_analog_write">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN">~</field>
                </shadow>
            </value>
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
    </category>
    <category id="category_arduino_time" name="%{BKY_ARDUINO_TIME_CATEGORY}" categorystyle="arduino_time_category">
        <block type="arduino_delay">
            <value name="DELAY_TIME">
                <shadow type="math_number">
                    <field name="NUM">1000</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_delay_microseconds">
            <value name="DELAY_TIME">
                <shadow type="math_number">
                    <field name="NUM">1000</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_millis"></block>
        <block type="arduino_micros"></block>
    </category>
    <category id="category_arduino_serial" name="%{BKY_ARDUINO_SERIAL_CATEGORY}" categorystyle="arduino_serial_category">
        <block type="arduino_serial_begin"></block>
        <block type="arduino_serial_print">
            <value name="CONTENT">
                <shadow type="text">
                    <field name="TEXT">Hello, World!</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_serial_println">
            <value name="CONTENT">
                <shadow type="text">
                    <field name="TEXT">Hello, World!</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_serial_available"></block>
        <block type="arduino_serial_read"></block>
    </category>
</category>
<category id="category_picar" name="%{BKY_PICAR_CATEGORY}" categorystyle="picar_category">
    <block type="picar_init"></block>
    <block type="picar_resetPiCar"></block>
    <block type="picar_drive">
        <value name="POWER_L">
            <shadow type="math_number">
                <field name="NUM">255</field>
            </shadow>
        </value>
        <value name="POWER_R">
            <shadow type="math_number">
                <field name="NUM">255</field>
            </shadow>
        </value>
    </block>
    <block type="picar_stop"></block>
    <block type="picar_checkDistance"></block>
    <block type="picar_checkColor"></block>
    <block type="picar_checkGray"></block>
    <block type="picar_inPosition"></block>
    <block type="picar_set_hand_range">
        <value name="RANGE">
            <shadow type="math_number">
                <field name="NUM">170</field>
            </shadow>
        </value>
    </block>
    <block type="picar_move_hands">
        <value name="PERCENT">
            <shadow type="math_number">
                <field name="NUM">50</field>
            </shadow>
        </value>
        <value name="SPEED">
            <shadow type="math_number">
                <field name="NUM">8</field>
            </shadow>
        </value>
    </block>
    <block type="picar_set_led_color"></block>
    <block type="picar_flashingLight"></block>
    <block type="picar_set_tempo">
        <value name="BPM">
            <shadow type="math_number">
                <field name="NUM">120</field>
            </shadow>
        </value>
    </block>
    <block type="picar_play_note">
        <value name="PIN">
            <shadow type="math_number">
                <field name="NUM">22</field>
            </shadow>
        </value>
        <value name="FREQUENCY">
            <shadow type="picar_note_to_frequency">
                <field name="NOTE_NAME">C</field>
                <field name="OCTAVE">4</field>
            </shadow>
        </value>
    </block>
    <block type="picar_note_to_frequency"></block>
    <block type="picar_tone">
        <value name="PIN">
            <shadow type="math_number">
                <field name="NUM">22</field>
            </shadow>
        </value>
        <value name="FREQUENCY">
            <shadow type="math_number">
                <field name="NUM">440</field>
            </shadow>
        </value>
        <value name="DURATION">
            <shadow type="math_number">
                <field name="NUM">200</field>
            </shadow>
        </value>
    </block>
    <block type="picar_no_tone">
        <value name="PIN">
            <shadow type="math_number">
                <field name="NUM">22</field>
            </shadow>
        </value>
    </block>
</category>
<category id="category_coding" name="%{BKY_CODING_CATEGORY}" categorystyle="coding_category">
    <block type="coding_raw_statement"></block>
    <block type="coding_raw_input"></block>
    <block type="coding_raw_definition"></block>
    <block type="coding_raw_wrapper"></block>
</category>
<sep></sep>
<category id="category_logic" name="%{BKY_LOGIC_CATEGORY}" categorystyle="logic_category">
    <block type="controls_if"></block>
    <block type="logic_compare"></block>
    <block type="logic_operation"></block>
    <block type="logic_negate"></block>
    <block type="logic_boolean"></block>
</category>
<category id="category_loops" name="%{BKY_LOOPS_CATEGORY}" categorystyle="loop_category">

    <block type="controls_while"></block>
    <block type="controls_for">
        <value name="FROM">
            <shadow type="math_number">
                <field name="NUM">1</field>
            </shadow>
        </value>
        <value name="TO">
            <shadow type="math_number">
                <field name="NUM">10</field>
            </shadow>
        </value>
        <value name="BY">
            <shadow type="math_number">
                <field name="NUM">1</field>
            </shadow>
        </value>
    </block>
    <block type="controls_flow_statements"></block>
</category>
<category id="category_math" name="%{BKY_MATH_CATEGORY}" categorystyle="math_category">
    <block type="math_number"></block>
    <block type="math_arithmetic"></block>
    <block type="math_single"></block>
    <block type="arduino_constrain">
        <value name="VALUE">
            <shadow type="variables_get"></shadow>
        </value>
        <value name="LOW">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
        <value name="HIGH">
            <shadow type="math_number">
                <field name="NUM">255</field>
            </shadow>
        </value>
    </block>
    <block type="arduino_map">
        <value name="VALUE">
            <shadow type="variables_get"></shadow>
        </value>
        <value name="FROMLOW">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
        <value name="FROMHIGH">
            <shadow type="math_number">
                <field name="NUM">1023</field>
            </shadow>
        </value>
        <value name="TOLOW">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
        <value name="TOHIGH">
            <shadow type="math_number">
                <field name="NUM">255</field>
            </shadow>
        </value>
    </block>
    <block type="math_random_seed">
        <value name="SEED">
            <shadow type="arduino_analog_read">
                <value name="PIN">
                    <shadow type="arduino_pin_shadow">
                        <field name="PIN">A0</field>
                    </shadow>
                </value>
            </shadow>
        </value>
    </block>
    <block type="math_random_int">
        <value name="MIN">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
        <value name="MAX">
            <shadow type="math_number">
                <field name="NUM">100</field>
            </shadow>
        </value>
    </block>
</category>
<category id="category_text" name="%{BKY_TEXT_CATEGORY}" categorystyle="text_category">
    <block type="text"></block>
    <block type="text_join"></block>
    <block type="text_append">
        <value name="TEXT">
            <shadow type="text"></shadow>
        </value>
    </block>
    <block type="text_length"></block>
</category>

<sep></sep>
<category id="category_variables" name="%{BKY_VARIABLES_CATEGORY}" categorystyle="variable_category">
    <button text="%{BKY_NEW_VARIABLE}" callbackKey="CREATE_VARIABLE"></button>
    <block type="variables_declare_global"></block>
    <block type="variables_declare_local"></block>
    <block type="variables_get"></block>
    <block type="variables_set"></block>
</category>

<category id="category_array" name="%{BKY_ARRAY_CATEGORY}" categorystyle="array_category">
    <block type="array_declare_global">
        <value name="SIZE">
            <shadow type="math_number">
                <field name="NUM">10</field>
            </shadow>
        </value>
    </block>
    <block type="array_declare_local">
        <value name="SIZE">
            <shadow type="math_number">
                <field name="NUM">10</field>
            </shadow>
        </value>
    </block>
    <block type="array_get">
        <value name="INDEX">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
    </block>
    <block type="array_set">
        <value name="INDEX">
            <shadow type="math_number">
                <field name="NUM">0</field>
            </shadow>
        </value>
    </block>
    <block type="array_length"></block>
</category>

<category id="category_functions" name="%{BKY_FUNCTIONS_CATEGORY}" categorystyle="procedure_category">
        <block type="custom_functions_defnoreturn"></block>
        <block type="custom_functions_defreturn"></block>
        <block type="custom_functions_return"></block>
        <block type="custom_functions_callnoreturn_manual"></block>
        <block type="custom_functions_callreturn_manual"></block>
    </category>
<sep></sep>
