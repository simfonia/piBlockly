<category id="category_simfonia" name="%{BKY_SIMFONIA_CATEGORY}" colour="%{BKY_SIMFONIA_HUE}">
    <category id="category_arduino" name="%{BKY_ARDUINO_CATEGORY}" colour="%{BKY_ARDUINO_HUE}">
        <block type="initializes_setup"></block>
        <block type="initializes_loop"></block>
        <block type="arduino_pin_mode">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
            <value name="MODE">
                <shadow type="arduino_pin_mode_mode_shadow"></shadow>
            </value>
        </block>
        <block type="arduino_pin_shadow"></block>
        <block type="arduino_pin_mode_mode_shadow"></block>
        <block type="arduino_digital_read">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
        </block>
        <block type="arduino_digital_write">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN"></field>
                </shadow>
            </value>
        </block>
        <block type="arduino_analog_read">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN">A0</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_analog_write">
            <value name="PIN">
                <shadow type="arduino_pin_shadow">
                    <field name="PIN">~</field>
                </shadow>
            </value>
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_constrain">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">變數</field>
                </shadow>
            </value>
            <value name="LOW">
                <shadow type="math_number">
                    <field name="NUM">下限</field>
                </shadow>
            </value>
            <value name="HIGH">
                <shadow type="math_number">
                    <field name="NUM">上限</field>
                </shadow>
            </value>
        </block>
        <block type="arduino_map">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">變數</field>
                </shadow>
            </value>
            <value name="FROMLOW">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="FROMHIGH">
                <shadow type="math_number">
                    <field name="NUM">1023</field>
                </shadow>
            </value>
            <value name="TOLOW">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="TOHIGH">
                <shadow type="math_number">
                    <field name="NUM">255</field>
                </shadow>
            </value>
        </block>
    </category>
    <category id="category_picar" name="%{BKY_PICAR_CATEGORY}" colour="%{BKY_PICAR_HUE}">
        <block type="picar_init"></block>
        <block type="picar_resetPiCar"></block>
        <block type="picar_drive">
            <value name="POWER_L">
                <shadow type="math_number">
                    <field name="NUM">255</field>
                </shadow>
            </value>
            <value name="POWER_R">
                <shadow type="math_number">
                    <field name="NUM">255</field>
                </shadow>
            </value>
        </block>
        <block type="picar_stop"></block>
        <block type="picar_checkDistance"></block>
        <block type="picar_checkColor"></block>
        <block type="picar_checkGray"></block>
        <block type="picar_inPosition"></block>
