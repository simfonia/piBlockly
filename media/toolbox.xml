        <block type="picar_openHands"></block> -->
        <block type="picar_set_hand_range">
            <value name="RANGE">
                <shadow type="math_number">
                    <field name="NUM">170</field>
                </shadow>
            </value>
        </block>
        <block type="picar_move_hands">
            <value name="PERCENT">
                <shadow type="math_number">
                    <field name="NUM">50</field>
                </shadow>
            </value>
            <value name="SPEED">
                <shadow type="math_number">
                    <field name="NUM">8</field>
                </shadow>
            </value>
        </block>
        <block type="picar_set_led_color"></block>
        <block type="picar_flashingLight"></block>
        <block type="picar_easterEgg">
            <value name="TEMPO">
                <shadow type="math_number">
                    <field name="NUM">1000</field>
                </shadow>
            </value>
        </block>
        <block type="picar_tone">
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">22</field>
                </shadow>
            </value>
            <value name="FREQUENCY">
                <shadow type="math_number">
                    <field name="NUM">440</field>
                </shadow>
            </value>
            <value name="DURATION">
                <shadow type="math_number">
                    <field name="NUM">200</field>
                </shadow>
            </value>
        </block>
        <block type="picar_no_tone">
            <value name="PIN">
                <shadow type="math_number">
                    <field name="NUM">22</field>
                </shadow>
            </value>
        </block>
    </category>
    <category id="category_coding" name="%{BKY_CODING_CATEGORY}" colour="%{BKY_CODING_HUE}">
        <block type="coding_raw_statement"></block>
        <block type="coding_raw_input"></block>
        <block type="coding_raw_definition"></block>
        <block type="coding_raw_wrapper"></block>
    </category>
</category>
<sep></sep>
<category name="Logic" colour="#5C81A6">
    <block type="controls_if"></block>
    <block type="logic_compare"></block>
    <block type="logic_operation"></block>
    <block type="logic_negate"></block>
    <block type="logic_boolean"></block>
</category>
<category name="Loops" colour="#5CA65C">
    <block type="controls_repeat_ext">
        <value name="TIMES">
            <shadow type="math_number">
                <field name="NUM">10</field>
            </shadow>
        </value>
    </block>
    <block type="controls_whileUntil"></block>
    <block type="controls_for">
        <value name="FROM">
            <shadow type="math_number">
                <field name="NUM">1</field>
            </shadow>
        </value>
        <value name="TO">
            <shadow type="math_number">
                <field name="NUM">10</field>
            </shadow>
        </value>
        <value name="BY">
            <shadow type="math_number">
                <field name="NUM">1</field>
            </shadow>
        </value>
    </block>
    <block type="controls_flow_statements"></block>
</category>
<category name="Math" colour="#5C68A6">
    <block type="math_number"></block>
    <block type="math_arithmetic"></block>
    <block type="math_single"></block>
</category>
<category name="Text" colour="#A6745C">
    <block type="text"></block>
    <block type="text_join"></block>
    <block type="text_append">
        <value name="TEXT">
            <shadow type="text"></shadow>
        </value>
    </block>
</category>
<category name="Lists" colour="#745CA6">
    <block type="lists_create_with"></block>
    <block type="lists_repeat"></block>
    <block type="lists_length"></block>
    <block type="lists_isEmpty"></block>
    <block type="lists_indexOf"></block>
    <block type="lists_getIndex"></block>
    <block type="lists_setIndex"></block>
</category>
<sep></sep>
