        <block type="piblockly_hw_stepper_init"></block>
        <block type="piblockly_hw_stepper_set_speed"></block>
        <block type="piblockly_hw_stepper_set_accel"></block>
        <block type="piblockly_hw_stepper_move_to"></block>
        <block type="piblockly_hw_stepper_move"></block>
        <block type="piblockly_hw_stepper_run"></block>
        <block type="piblockly_hw_stepper_stop"></block>
      </category>
