        <block type="piblockly_hw_l293d_stepper_speed"></block>
        <block type="piblockly_hw_l293d_stepper_run"></block>
        <block type="piblockly_hw_l293d_stepper_stop"></block>
        <block type="piblockly_hw_l293d_servo_run"></block>
        <block type="piblockly_hw_l293d_servo_detach"></block>
