      <block type="piblockly_hw_ultrasonic_distance"></block>
      <block type="piblockly_hw_dht_read"></block>
    </category>
    <category name="%{BKY_PIBLOCKLY_HW_SENSORS_MPU9250}" id="PIBLOCKLY_HW_SENSORS_MPU9250" colour="#70A8FF">
