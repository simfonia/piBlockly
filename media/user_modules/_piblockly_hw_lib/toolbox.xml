        <block type="piblockly_hw_mpu9250_mag_3axis"></block>
        <block type="piblockly_hw_mpu9250_accel_pitch_roll"></block>
    </category>
    <category name="MAX30105" colour="#70A8FF">
      <block type="PIBLOCKLY_HW_max30105_init">
        <value name="LED_RED">
          <block type="math_number">
            <field name="NUM">50</field>
          </block>
        </value>
      </block>
      <block type="PIBLOCKLY_HW_max30105_check"></block>
      <block type="PIBLOCKLY_HW_max30105_get_beat_rate">
        <value name="AVG">
          <block type="math_number">
            <field name="NUM">4</field>
          </block>
        </value>
      </block>
      <block type="PIBLOCKLY_HW_max30105_get_spo2"></block>
      <block type="PIBLOCKLY_HW_max30105_get_temperature"></block>
      <block type="PIBLOCKLY_HW_max30105_set_beat_range">
        <value name="MIN">
          <block type="math_number">
            <field name="NUM">20</field>
          </block>
        </value>
        <value name="MAX">
          <block type="math_number">
            <field name="NUM">250</field>
          </block>
        </value>
      </block>
      <block type="PIBLOCKLY_HW_max30105_set_spo2_clear"></block>
    </category>
    <category name="PS2" colour="#70A8FF">
      <block type="PIBLOCKLY_HW_ps2_initial">
        <value name="CLK">
          <block type="math_number">
            <field name="NUM">13</field>
          </block>
        </value>
        <value name="CMD">
          <block type="math_number">
            <field name="NUM">11</field>
          </block>
        </value>
        <value name="CS">
          <block type="math_number">
            <field name="NUM">10</field>
          </block>
        </value>
        <value name="DAT">
          <block type="math_number">
            <field name="NUM">12</field>
          </block>
        </value>
      </block>
      <block type="PIBLOCKLY_HW_ps2_read">
        <value name="val">
          <block type="math_number">
            <field name="NUM">0</field>
          </block>
        </value>
      </block>
      <block type="PIBLOCKLY_HW_ps2_button_event"></block>
      <block type="PIBLOCKLY_HW_ps2_stick_direction"></block>
      <block type="PIBLOCKLY_HW_ps2_analog_read"></block>
      <block type="PIBLOCKLY_HW_ps2_pressures_read"></block>
